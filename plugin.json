{
  "version": "0.0.3",
  "name": "Etsy",
  "description": "Plentymarkets integration for the Etsy marketplace",
  "namespace": "Etsy",
  "pluginIcon":"icon_plugin_xs.png",
  "author": "plentymarkets GmbH",
  "email":"support@plentymarkets.eu",
  "authorIcon":"icon_author_xs.png",
  "shortDescription": "Plugin für Etsy-Marktplatzintegartion. Um Etsy in plentymarkets nutzen zu können, ist eine Händlerregistrierung bei Etsy nötig.",
  "license":"AGPL-3.0",
  "categories": ["Plugins/Backend"],
  "price":0.00,
  "type": "general",
  "serviceProvider": "Etsy\\EtsyServiceProvider",
  "dependencies": {
  	"inakiabt/etsy-php": "dev-master"
  },
  "runOnBuild": [
    "Etsy\\Migrations\\CreatePaymentMethod",
    "Etsy\\Migrations\\CreateSettingsTable",
    "Etsy\\Migrations\\CreateVariationImageTable",
    "Etsy\\Migrations\\CreateOrderReferrer",
    "Etsy\\Migrations\\CreateMarketPropertySettings"
  ]
}